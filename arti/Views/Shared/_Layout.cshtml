<head>
    @Html.Partial("Head")
</head>
<body>
    <div id="container">
        <div id="wrap">
            @Html.Partial("Header")
            <div id="main" role="main">
                @RenderBody()
            </div>
        </div>
        @Html.Partial("Footer")
    </div>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="@Url.Content("~/Scripts/Libs/jquery-1.7.2.min.js")"><\\/script>')</script>
        
    <script src="@Url.Content("~/Scripts/Libs/processing-1.3.6.min.js")"></script>
    <script defer src="@Url.Content("~/Scripts/plugins.js")"></script>
    <script defer src="@Url.Content("~/Scripts/script.js")"></script>

    @{
        bool isProcessing = ViewBag.IsProcessing != null && ViewBag.IsProcessing == true;
        string isProcessingStr = isProcessing.ToString().ToLower();
        var jsonArtifact = isProcessing ? Html.Raw(Json.Encode(Model)) : "{}";
    }

    <script>
            
        $(document).ready(function() {
            artifactData = '@jsonArtifact';
            ARTI.init('@ViewBag.Page', '@isProcessingStr');   
        });

