        <td>
            @Html.DisplayFor(modelItem => item.arm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.leg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bbox)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.frameSize)
        </td>
