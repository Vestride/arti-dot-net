@model IEnumerable<arti.Models.Artifact>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            timestamp
        </th>
        <th>
            wingspan
        </th>
        <th>
            head
