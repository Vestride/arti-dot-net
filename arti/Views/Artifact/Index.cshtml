        <th>
            rhandX
        </th>
        <th>
            rhandY
        </th>
        <th>
            torso
        </th>
        <th>
            height
        </th>
        <th>
            arm
        </th>
        <th>
            leg
        </th>
        <th>
            bbox
        </th>
        <th>
            frameSize
        </th>
        <th>
            views
        </th>
        <th>
            stars
        </th>
        <th>
            processed
        </th>
        <th>
            owner
        </th>
